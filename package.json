    "db:sqlite:reset": "prisma migrate reset --force --schema=prisma/schema.sqlite.prisma && prisma db seed --schema=prisma/schema.sqlite.prisma",
    "db:studio:sqlite": "prisma studio --schema=prisma/schema.sqlite.prisma"
  },
